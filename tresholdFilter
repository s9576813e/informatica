void tresholdFilterP2(ImageP2* input, unsigned int treshold){
    for(int r=0; r<input->size.rows; r++){
        for(int c=0; c<input->size.cols; c++){
            if(input->data.pixels[r][c] > treshold){
                input->data.pixels[r][c] = 1;
             
              }
               else {
                input->data.pixels[r][c] = 0;
            }
        }
    }
}
